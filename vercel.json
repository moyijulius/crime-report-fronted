{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://your-backend-app.vercel.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "build/$1"
    }
  ]
}